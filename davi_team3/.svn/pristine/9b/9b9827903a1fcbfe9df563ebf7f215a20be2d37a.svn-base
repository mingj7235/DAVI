package com.davi.app.board.vo;

/*CHEATNUM NUMBER(10),  --게시글 번호
CHEATREPLYNUM NUMBER(10),  -- 댓글번호
PARENTREPLYNUM NUMBER(10) DEFAULT 0 NOT NULL, --부모 댓글의 번호 부모가없으면0
MEMBERID VARCHAR2(300) NOT NULL,
CHEATREPLYCONTENT VARCHAR2(3000),
CHEATREPLYDATE DATE,*/

public class BoardCheatReplyVO {
	private int cheatNum;
	private int cheatReplyNum;
	private int parentReplyNum;
	private String memberId;
	private String cheatReplyContent;
	private String cheatReplyDate;
	private String parentReplyId;
	
	public BoardCheatReplyVO() {;}

	public int getCheatNum() {
		return cheatNum;
	}

	public void setCheatNum(int cheatNum) {
		this.cheatNum = cheatNum;
	}

	public int getCheatReplyNum() {
		return cheatReplyNum;
	}

	public void setCheatReplyNum(int cheatReplyNum) {
		this.cheatReplyNum = cheatReplyNum;
	}

	public int getParentReplyNum() {
		return parentReplyNum;
	}

	public void setParentReplyNum(int parentReplyNum) {
		this.parentReplyNum = parentReplyNum;
	}

	public String getMemberId() {
		return memberId;
	}

	public void setMemberId(String memberId) {
		this.memberId = memberId;
	}

	public String getCheatReplyContent() {
		return cheatReplyContent;
	}

	public void setCheatReplyContent(String cheatReplyContent) {
		this.cheatReplyContent = cheatReplyContent;
	}

	public String getCheatReplyDate() {
		return cheatReplyDate;
	}

	public void setCheatReplyDate(String cheatReplyDate) {
		this.cheatReplyDate = cheatReplyDate;
	}

	public String getParentReplyId() {
		return parentReplyId;
	}

	public void setParentReplyId(String parentReplyId) {
		this.parentReplyId = parentReplyId;
	}


	
}
